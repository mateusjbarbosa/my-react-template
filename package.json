{
  "name": "mateusjbarbosa.dev",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "eslint": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write .",
    "lint": "pnpm prettier && pnpm eslint",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "jest --runInBand",
    "test:watch": "pnpm jest --watchAll --coverage",
    "test:staged": "pnpm jest --findRelatedTests --passWithNoTests"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@swc/core": "1.3.78",
    "@swc/jest": "0.2.29",
    "@testing-library/jest-dom": "6.0.1",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.3",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "@vitejs/plugin-react-swc": "3.3.2",
    "autoprefixer": "10.4.15",
    "eslint": "8.47.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "jest": "29.6.3",
    "jest-environment-jsdom": "29.6.3",
    "lint-staged": "14.0.1",
    "postcss": "8.4.28",
    "prettier": "3.0.2",
    "prettier-plugin-tailwindcss": "0.5.3",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite": "4.4.9"
  }
}
